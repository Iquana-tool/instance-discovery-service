# Completion Segmentation Service Environment Variables
# Copy this file to .env for local development

# Hugging Face Access Token (required for SAM3 model downloads)
HF_ACCESS_TOKEN=your_huggingface_token_here

# Allowed CORS origins
ALLOWED_ORIGINS=http://localhost:8000,http://localhost:3000

# Port (default: 8003)
PORT=8003

# Log directory
LOG_DIR=logs
